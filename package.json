{
  "name": "@big-whale-labs/seal-cred-email",
  "version": "0.1.3",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --skipLibCheck",
    "distribute": "node dist/server.js",
    "start": "tsc-watch --skipLibCheck --onSuccess 'node dist/server.js'",
    "lint": "prettier --check src && eslint --max-warnings 0 --ext ts scripts && yarn eth-lint",
    "prettify": "prettier --write src",
    "release": "np",
    "prepublishOnly": "yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "np": {
    "2fa": false,
    "tests": false,
    "cleanup": false,
    "yarn": false
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.17.13",
    "@types/mjml-react": "^2.0.4",
    "express": "^4.16.4",
    "nodemon": "^1.18.9",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "mjml": "^4.3.1",
    "mjml-react": "^1.0.45",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "packageManager": "yarn@3.2.1",
  "description": "SealCred email templates",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigWhaleLabs/seal-cred-email.git"
  },
  "keywords": [
    "emails"
  ],
  "bugs": {
    "url": "https://github.com/BigWhaleLabs/seal-cred-email/issues"
  },
  "homepage": "https://github.com/BigWhaleLabs/seal-cred-email#readme"
}
